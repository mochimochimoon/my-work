<header class="header">
  <div class="header-action">
    <p class="header-btn">
        <button class=" js-menu-trigger" data-active-class="is-menu-active-side">
          <span class="header-btn_icon"><% include /ejs/_icon/_humberger %></span>
          <span class="header-btn_icon header-btn_icon--close"><% include /ejs/_icon/_close %></span>
        </button>
    </p>
    <ul class="header-link">
      <li class="header-btn">
        <button class=" js-menu-trigger" data-active-class="is-menu-active-modal">
          <span class="header-btn_icon"><% include /ejs/_icon/_report %></span>
          <span class="header-btn_icon header-btn_icon--close"><% include /ejs/_icon/_close %></span>
        </button>
      </li>
      <li class="header-btn header-btn--down">
        <button class="js-menu-trigger" data-active-class="is-menu-active-down">
          <% include /ejs/_icon/_down-arrow %>
        </button>
      </li>
    </ul>
  </div>
</header>
